<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/activity_padding_medium"
        android:layout_marginEnd="@dimen/activity_padding_medium"
        android:orientation="vertical"
        tools:ignore="UselessParent">
        <TextView
            android:id="@+id/kudos_belief_name_tv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="start|center"
            android:fontFamily="@font/roboto_medium"
            android:paddingStart="@dimen/activity_padding_medium"
            android:paddingEnd="@dimen/activity_padding_medium"
            android:paddingBottom="@dimen/activity_margin_quarter"
            android:singleLine="true"
            android:text="kudos name"
            android:textAlignment="textStart"
            android:textColor="@color/red"
            android:textSize="@dimen/text_size_medium"
            tools:ignore="TextContrastCheck" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="2.5"
            android:layout_gravity="center">
            <TextView
                android:id="@+id/kudos_value_name_tv"
                android:layout_width="0dp"
                android:layout_weight=".52"
                android:layout_height="wrap_content"
                android:layout_gravity="start|center"
                android:singleLine="true"
                android:paddingStart="@dimen/activity_padding_medium"
                android:paddingEnd="@dimen/activity_padding_medium"
                android:paddingTop="@dimen/activity_margin_quarter"
                android:paddingBottom="@dimen/activity_margin_quarter"
                android:fontFamily="@font/roboto_medium"
                android:text="Values"
                android:textAlignment="textStart"
                android:textColor="@color/border_color"
                android:textSize="@dimen/text_size_normal"
                />

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:paddingStart="@dimen/activity_padding_medium"
                android:paddingEnd="@dimen/activity_padding_medium"
                android:paddingTop="@dimen/activity_margin_quarter"
                android:paddingBottom="@dimen/activity_margin_quarter"
                android:gravity="center"
                android:orientation="horizontal"
                android:layout_weight=".7">
                <TextView
                    android:id="@+id/org_value_current_tv"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center|end"
                    android:fontFamily="@font/roboto_medium"
                    android:singleLine="true"
                    android:textAlignment="textEnd"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_small"
                    tools:ignore="NestedWeights,TextContrastCheck" />
                <TextView
                    android:id="@+id/org_value_difference_tv"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:layout_gravity="start|center"
                    android:fontFamily="@font/roboto_medium"
                    android:singleLine="true"
                    android:textAlignment="textStart"
                    android:textColor="@color/red"
                    android:textSize="@dimen/text_size_small"
                    tools:ignore="TextContrastCheck" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:paddingStart="@dimen/activity_padding_medium"
                android:paddingEnd="@dimen/activity_padding_medium"
                android:paddingTop="@dimen/activity_margin_quarter"
                android:paddingBottom="@dimen/activity_margin_quarter"
                android:gravity="center"
                android:orientation="horizontal"
                android:layout_weight=".66">
                <TextView
                    android:id="@+id/team_value_current_tv"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center|end"
                    android:fontFamily="@font/roboto_medium"
                    android:singleLine="true"
                    android:textAlignment="textEnd"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_small"
                    tools:ignore="NestedWeights,TextContrastCheck" />
                <TextView
                    android:id="@+id/team_value_difference_tv"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:layout_gravity="start|center"
                    android:fontFamily="@font/roboto_medium"
                    android:singleLine="true"
                    android:textAlignment="textStart"
                    android:textColor="@color/red"
                    android:textSize="@dimen/text_size_small"
                    tools:ignore="TextContrastCheck" />
            </LinearLayout>
            <LinearLayout

                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:paddingStart="@dimen/activity_padding_medium"
                android:paddingEnd="@dimen/activity_padding_medium"
                android:paddingTop="@dimen/activity_margin_quarter"
                android:paddingBottom="@dimen/activity_margin_quarter"
                android:gravity="center|end"
                android:orientation="horizontal"
                android:layout_weight=".6">
                <TextView
                    android:id="@+id/your_current_value_tv"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center|end"
                    android:fontFamily="@font/roboto_medium"
                    android:singleLine="true"
                    android:textAlignment="textEnd"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_small"
                    tools:ignore="NestedWeights,TextContrastCheck" />
                <TextView
                    android:id="@+id/your_difference_value_tv"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:layout_gravity="start|center"
                    android:fontFamily="@font/roboto_medium"
                    android:singleLine="true"
                    android:textAlignment="textStart"
                    android:textColor="@color/red"
                    android:textSize="@dimen/text_size_small"
                    tools:ignore="TextContrastCheck" />
            </LinearLayout>


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="2.5"
            android:layout_gravity="center">
            <TextView
                android:id="@+id/kudos_award_name_tv"
                android:layout_width="0dp"
                android:layout_weight=".52"
                android:layout_height="wrap_content"
                android:layout_gravity="start|center"
                android:singleLine="true"
                android:paddingStart="@dimen/activity_padding_medium"
                android:paddingEnd="@dimen/activity_padding_medium"
                android:paddingTop="@dimen/activity_margin_quarter"
                android:paddingBottom="@dimen/activity_margin_quarter"
                android:fontFamily="@font/roboto_medium"
                android:text="Awards"
                android:textAlignment="textStart"
                android:textColor="@color/border_color"
                android:textSize="@dimen/text_size_normal"
                />

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:paddingStart="@dimen/activity_padding_medium"
                android:paddingEnd="@dimen/activity_padding_medium"
                android:paddingTop="@dimen/activity_margin_quarter"
                android:paddingBottom="@dimen/activity_margin_quarter"
                android:gravity="center"
                android:orientation="horizontal"
                android:layout_weight=".7">
                <TextView
                    android:id="@+id/org_awards_current_tv"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center|end"
                    android:fontFamily="@font/roboto_medium"
                    android:singleLine="true"
                    android:text="12"
                    android:textAlignment="textEnd"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_small"
                    tools:ignore="NestedWeights,TextContrastCheck" />
                <TextView
                    android:id="@+id/org_awards_difference_tv"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:layout_gravity="start|center"
                    android:fontFamily="@font/roboto_medium"
                    android:singleLine="true"
                    android:text="(-2)"
                    android:textAlignment="textStart"
                    android:textColor="@color/red"
                    android:textSize="@dimen/text_size_small"
                    tools:ignore="TextContrastCheck" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:paddingStart="@dimen/activity_padding_medium"
                android:paddingEnd="@dimen/activity_padding_medium"
                android:paddingTop="@dimen/activity_margin_quarter"
                android:paddingBottom="@dimen/activity_margin_quarter"
                android:gravity="center"
                android:orientation="horizontal"
                android:layout_weight=".66">
                <TextView
                    android:id="@+id/team_awards_current_tv"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center|end"
                    android:fontFamily="@font/roboto_medium"
                    android:singleLine="true"
                    android:text="6"
                    android:textAlignment="textEnd"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_small"
                    tools:ignore="NestedWeights,TextContrastCheck" />
                <TextView
                    android:id="@+id/team_awards_difference_tv"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:layout_gravity="start|center"
                    android:fontFamily="@font/roboto_medium"
                    android:singleLine="true"
                    android:text="(-3)"
                    android:textAlignment="textStart"
                    android:textColor="@color/red"
                    android:textSize="@dimen/text_size_small"
                    tools:ignore="TextContrastCheck" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:paddingStart="@dimen/activity_padding_medium"
                android:paddingEnd="@dimen/activity_padding_medium"
                android:paddingTop="@dimen/activity_margin_quarter"
                android:paddingBottom="@dimen/activity_margin_quarter"
                android:gravity="center|end"
                android:orientation="horizontal"
                android:layout_weight=".6">
                <TextView
                    android:id="@+id/your_current_awards_tv"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center|end"
                    android:fontFamily="@font/roboto_medium"
                    android:singleLine="true"
                    android:text="0"
                    android:textAlignment="textEnd"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_small"
                    tools:ignore="NestedWeights,TextContrastCheck" />
                <TextView
                    android:id="@+id/your_difference_awards_tv"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:layout_gravity="start|center"
                    android:fontFamily="@font/roboto_medium"
                    android:singleLine="true"
                    android:text="(-1)"
                    android:textAlignment="textStart"
                    android:textColor="@color/red"
                    android:textSize="@dimen/text_size_small"
                    tools:ignore="TextContrastCheck" />
            </LinearLayout>


        </LinearLayout>
        <View
            android:id="@+id/bottom_view"
            android:layout_width="match_parent"
            android:visibility="gone"
            android:layout_height=".5dp"
            android:paddingStart="@dimen/activity_margin"
            android:paddingEnd="@dimen/activity_margin"
            android:background="@color/edit_bg"
            android:layout_marginTop="@dimen/activity_padding_medium"
            android:layout_marginStart="@dimen/activity_margin"
            android:layout_marginEnd="@dimen/activity_padding_medium"
            android:layout_marginBottom="@dimen/activity_padding_medium"/>
    </LinearLayout>


</LinearLayout>